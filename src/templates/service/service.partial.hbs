{{#if print_jsdocs}}
/**
 * Abstract class representing a service for ...
 *
 * @abstract
 * @class {{pascal_case name}}Service
 */
{{/if}}
{{#if use_ioc}}
@injectable()
{{/if}}
export abstract class {{pascal_case name}}Service {
  public static Token = '{{constant_case name}}_SERVICE';

  {{#each methods}}
  {{> abstract_method this}}
  {{/each}}
}